{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "$lib/*": ["src/lib/*"]
    },
    "rootDir": "src/lib/core",
    "outDir": ".golden_build_cjs",
    "strict": true,
    "skipLibCheck": true,
    "noEmitOnError": true
  },
  "include": ["src/lib/core/**/*.ts"]
}
